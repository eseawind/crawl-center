<?xml version="1.0" encoding="UTF-8"?>
<Root>
	<Database>
		<DB>
			<DriverClass>${Maven.Database.DB.DriverClass}</DriverClass>
			<ConnectionStr>${Maven.Database.DB.ConnectionStr}</ConnectionStr>
			<Username>${Maven.Database.DB.Username}</Username>
			<Password>${Maven.Database.DB.Password}</Password>
		</DB>
		<MongoDB>
			<Ip>${Maven.MongoDB.Ip}</Ip>
			<Port>${Maven.MongoDB.Port}</Port>
			<DatabaseName>${Maven.MongoDB.DatabaseName}</DatabaseName>
			<AdminDatabaseName>${Maven.MongoDB.AdminDatabaseName}</AdminDatabaseName>
		</MongoDB>
	</Database>

	<Executor>
		<NumThreads>200</NumThreads>
		<MaxQueueSize>200</MaxQueueSize>
	</Executor>

	<Crawl>
		<SleepingTime>10000</SleepingTime>
	</Crawl>

	<Collector>
		<BackupEndName>.bak</BackupEndName>
		<Threshold>1</Threshold>
	</Collector>

	<Log>
		<ShowResponseObject>
			<Enabled>true</Enabled>
		</ShowResponseObject>
		<Machine>${Log.Machine}</Machine>
	</Log>

	<IsTestEnvironment>false</IsTestEnvironment>
</Root>